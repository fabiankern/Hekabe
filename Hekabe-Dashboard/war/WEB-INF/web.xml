<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
	PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
	"http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
	<servlet>
	    <servlet-name>communicationServlet</servlet-name>
    	<servlet-class>com.hekabe.dashboard.server.CommunicationServiceImpl</servlet-class>
  	</servlet>
  
  	<servlet-mapping>
	    <servlet-name>communicationServlet</servlet-name>
	    <url-pattern>/com.hekabe.dashboard.Dashboard/comm</url-pattern>
  	</servlet-mapping>

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>Dashboard.html</welcome-file>
	</welcome-file-list>

</web-app>
